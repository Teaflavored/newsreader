<form>
    <label for="feed_title">Title</label>
    <input type="text"
           name="feed[title]"
           value="<%= feed.escape('title') %>"
           id="feed_title">
    <label for="feed_url">Url</label>
    <input type="text"
           name="feed[url]"
           value="<%= feed.escape('url') %>"
           id="feed_url">
        
    <input type="submit" value="Create a new feed!">
</form>


